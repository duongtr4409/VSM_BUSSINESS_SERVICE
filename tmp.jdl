entity StepInProcess {	// dữ liệu các bước quy trình
	id Long,
    stepOrder Long, 				// stt của bước trong quy trình
    stepInProcessCode String,
    stepInProcessName String,
    processingTerm Double 			// hạn xử lý từ thời gian bắt đầu bước quy trình
    processingActiveTime Instant, 	// thời gian bước xử lý bắt đầu được active (quy trình chuyển đến bưóc này)
    processingTermTime Instant,		// hạn xử lý sau khi tính toán từ processingTerm
    isRequiredSignature Boolean, 	// Có yêu cầu ký số ko: True: có, False: không
    isBack Boolean, 				// Có cho phép trả lại hay không: True: có, False: không
    isAuthorizedApproval Boolean, 	// có cho phép duyệt ủy quyền hay không: True: có, False: không
    isExamine Boolean,				// có cho phép gửi yêu cầu soát xét hay không
	isConsult Boolean,				// có cho phép gửi yêu cầu xin ý kiến hay không
    isEdit Boolean,					// có cho phép cán bộ phê duyệt chỉnh sửa hay không
    isRequestModify Boolean, 		// có cho phép yêu cầu sửa đổi bổ sung không
	isSendMail Boolean,				// có cho phép gửi mail hay không (nút bấm)
	isReSend Boolean,				// có cho phép gửi lại hay không

	isAutoSendMail Boolean,			// có gửi mail tự động hay không (mẫu mail sẽ theo trạng thái tiếp theo của phiếu (một trạng thái sẽ có 1 mẫu mail))
	isAttachFilePDF Boolean,		// có đính kèm file dạng PDF vào mail hay không	
		
	isSendNoticePriority Boolean,	// có gửi thông báo khẩn, cấp tốc hay không (nút bấm)
	sendNoticePriorityType String,	// mẫu mail khẩn sẽ gửi (lưu id tham chiếu đến bảng MailTemplate)
	
	isCreateOTPLink Boolean,		// có tạo OTP và link không 
	isSendOTP Boolean,				// có gửi mã OTP không
	
	isExportPDF Boolean,			// có cho xuất file PDF không (nút bấm)
	isAddQRCode Boolean,			// có thêm QRCode vào file ko
	isAddWaterMark Boolean,			// có thêm watterMark vào file ko
	isAddHistory Boolean,			// có thêm trang lịch sử xử lý vào file ko
	
	isProcessSaveDoc Boolean,		// có xử lý, lưu trữ công văn
	isChangeProcess Boolean,		// có chuyển người xử lý hay không (nút bấm)
	isSaveDoc Boolean,				// có lưu trữ văn bản hay không (nút bấm)
	isCreateReport Boolean,			// có tạo tờ trình không (nút bấm)
	isCreateOfficalDispath Boolean,	// có tạo công văn không (nút bấm)
	
	isCreateContract Boolean,		// có tạo hợp đồng không (nút bấm)
	contractType String,			// loại hợp đồng sẽ tạo (id của loại )
	processOfContract String,		// quy trình mà hợp đồng sẽ áp dụng
	createContractType String,		// kiểu tạo hợp đồng (1: tạo tất cả, 2: chỉ tạo những hợp đồng được phê duyệt)
	
	isProcessRecover Boolean,		// có cho phép xử lý yêu cầu thu hồi phiếu hay không
	isRecall Boolean,				// có cho phép thực hiện thu hồi phiếu hay không (nút bấm)
    isCreateTFSTask Boolean,		// có tạo task trên TFS hay không (nút bấm)
	
	isDeny Boolean,					// có cho phép từ chối không
	optionDeny Long,				// lựa chọn khi thực hiện từ chối (-1: về bước đầu, -2: tiếp tục, còn lại : chỉ định bước tiếp theo (sẽ là stt của bước))
	
	isSendMailCustomer Boolean,		// có gửi mail cho khách hàng không
	
	isSyncSap Boolean,				// cờ đánh dấu có đồng bộ hợp đồng sang SAP không
	
	isApprove Boolean,				// có được thực hiện phê duyệt không
	isManageStamp Boolean,			// quản lý đóng dấu hay không
	isChangeStatus Boolean,			// có cho phép thay đổi trạng thái hay không
	//isCreateContract Boolean,		// có tạo hợp đồng sau khi hoàn thành bước hay không
	//contractType Long				// loại hợp đồng tạo ra nếu cho phép tạo hợp đồng sau bước
	organizationCode String, 		// mã đơn xử lý
    organizationName String,		// tên đơn vị xử lý
    rankCode String,				// mã vai trò xử lý
    rankName String,				// tên vai trò xủ lý
    description String				// mô tả
    createdName String,
	createdOrgName String,			// tên đơn vị của người tạo
	createdRankName String,			// tên vai trò của người tạo
    createdDate Instant,
    modifiedName String,
    modifiedDate Instant,
    isActive Boolean,				// bước có được phép sử dụng không	
    isDelete Boolean,				// bước có bị xóa đi không 
	version Long,
	
                // phục vụ Saas
	tennantCode String, 		// mã đơn vị
    tennantName String,		// tên đơn vị
}

