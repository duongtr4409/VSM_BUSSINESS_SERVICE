entity UserInfo {
	id Long,
    name String,
	fullName String,
	address String,				// địa chỉ
	contactAddress String,		// địa chỉ liên hệ
	identification String,		// (giấy tờ nhận biết)căn cước công dân/ mã số thuế/ chứng minh thư/ ...
	issuseDate Instant,			// ngày cấp
	issuseOrg String,			// đơn vị cấp
	fax String,					// số fax
	avatar String,				// ảnh đại diện
	email String,				// địa chỉ mail
	userName String,			// tên tài khoản
	password String,			// mật khẩu
	passwordEncode String,		// mật khẩu mã hóa
	expiryDate Instant,			// thời hạn sử dụng
	numberOfLoginFailed Long,	// số lần đã đang nhập thất bại
	description String,			// mô tả
	isLocked Boolean,			// cờ đánh dấu tài khoản có đang khóa hay không
	userTypeName String,		// tên loại tài khoản
	userTypeCode String,		// mã loại tài khoản
	userType String,			// loại tài khoản (1: nhân viên VCR, 2: khách hàng)
	signTypeName String,		// tên lại hình ký
	signTypeCode String,		// mã loại hình ký
	signType String,			// loại hình ký (1: ký số, 2: ký HSM, ...)
	signFolder String,			// thông tin forder chứa thông tin ký mềm
    createdName String,
	createdOrgName String,		// tên đơn vị của người tạo
	createdRankName String,		// tên vai trò của người tạo
    createdDate Instant,
    modifiedName String,
    modifiedDate Instant,
	idInMicrosoft String, 		// id của user trong Microsoft 365
	oDataContext String,		// @odata.context trong Microsoft 365
	businessPhones String,		// businessPhones trong Microsoft 365 (nhiều phần  tử -> lưu dạng mảng) -> dùng JSON.parse() để lấy dữ
	displayName String,			// displayName trong Microsoft 365
	givenName String,			// givenName trong Microsoft 365
	jobTitle String,			// jobTitle trong Microsoft 365
	mail String,				// mail trong Microsoft 365
	mobilePhone String,			// mobilePhone trong Microsoft 365
	officeLocation String,		// officeLocation trong Microsoft 365
	preferredLanguage String,	// preferredLanguage trong Microsoft 365
	surname String,				// surname trong Microsoft 365
	userPrincipalName String,	// userPrincipalName trong Microsoft 365
	infoInMicrosoft TextBlob,		// thông tin trong Microsoft 365(dạng JSON)
    isActive Boolean,
    isDelete Boolean,
    
              // phục vụ Saas
	tennantCode String, 		// mã đơn vị
    tennantName String,		// tên đơn vị
}

