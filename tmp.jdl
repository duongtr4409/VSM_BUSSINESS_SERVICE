entity Field {	//  dữ liệu danh mục các trường trong form
	id Long,
    objectSchema TextBlob,
    fieldCode String,
    fieldName String,
    description String,		// mô tả
    createdName String,
	createdOrgName String,
	createdRankName String,
    createdDate Instant,
    modifiedName String,
    modifiedDate Instant,
    isActive Boolean,
    isDelete Boolean, 	// cờ đánh dấu Field có bị xóa đi không: True: đã xóa, False: chưa xóa
	version Long,
    		
            // phục vụ Saas
	tennantCode String, 		// mã đơn vị
    tennantName String,		// tên đơn vị
}

entity Form {	// dữ liệu danh mục các form (biểu mẫu)
	id Long,
    objectSchema TextBlob,		// lưu thông tin định dạng của Form (dạng Object Json Schema)
    objectModel TextBlob, 		// Lưu thông tin dữ liệu mặc định của Form(dữ liệu mặc định của các Field trong Form dạng Object Json)
    option TextBlob,			// lưu thông  tin cấu hình của Form
    FormCode String,			// mã Form 	
    FormName String,			// tên Form
	mappingInfo TextBlob,		// thông tin mapping (lưu dạng JSON)
    description String,			// mô tả
    createdName String,
	createdOrgName String,
	createdRankName String,
    createdDate Instant,
    modifiedName String,
    modifiedDate Instant,
    isActive Boolean,
    isDelete Boolean, 	// cờ đánh dấu Form có bị xóa đi không: True: đã xóa, False: chưa xóa
    version Long,
	
                // phục vụ Saas
	tennantCode String, 	// mã đơn vị
    tennantName String,		// tên đơn vị
}

entity FieldInForm {		// bảng móc nối quan hệ n-n Filed-Form
	id Long,
    fieldInFormCode String,		// mã trường dữ liệu
    fieldInFormName String,		// tên trường dữ liệu
    objectSchema TextBlob,			// lưu thông tin định dạng của Field trong Form (dạng Object Json Schema)
    objectModel TextBlob,			// lưu thông tin dữ liệu mặc định của Field trong Form (dạng Object Json)
    option TextBlob,				// lưu thông tin cấu hình tùy chọn của Field
    description String,				// mô tả
    nameOfField String,				// tên của trường dữ liệu trong Form
    fieldPattern String,			// dịnh dạng dữ liệu của trường dữ liệu
    required String,				// field có bắt buộc nhập không
	order Long,						// stt của field trong E-form
    createdName String,
	createdOrgName String,
	createdRankName String,
    createdDate Instant,
    modifiedName String,
    modifiedDate Instant,
	row Long,					// thứ tự hàng của field (ở hàng bao nhiêu) 
	col Long,					// thứ tự cột của field (ở cột bao nhiêu)
    
                // phục vụ Saas
	tennantCode String, 		// mã đơn vị
    tennantName String,		// tên đơn vị
}

entity Pattern {				// bảng danh mục mục các định dạng trường nhập liệu
	id Long,
	patternCode String, 			// mã định dạng
	patternName String,				// tên định dạng
	patternValue String,			// giá trị định dạng
	description String,				// mô tả
	createdName String,
	createdOrgName String,
	createdRankName String,
    createdDate Instant,
    modifiedName String,
    modifiedDate Instant,
	isActive Boolean,
	isDelete Boolean
}

